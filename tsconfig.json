{
    "compilerOptions": {
		"baseUrl": ".",
		"paths": { "*": ["types/*"] },
		"outDir": "./dist/",
        "sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule":true,
		"esModuleInterop": true,
        "noImplicitAny": true,
        "module": "commonJS",
        "target": "es6",
		"jsx": "react",
    },
    "include": [
        "./src/**/*"
    ]
}